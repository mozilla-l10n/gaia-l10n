# Download UI

stopped_download_title        = Lejupielāde apstājas
stopped_download_message      = Lejupielāde tika apstādināta. Vai vēlreiz mēģināt lejupielādēt?
stopped_download_left_button  = Atcelt
stopped_download_right_button = Mēģināt vēlreiz

failed_download_title         = Lejupielāde neizdevās
failed_download_message       = Neizdevās lejupielādēt {{name}}. Vai mēģināt to vēlreiz lejupielādēt?
failed_download_left_button   = Atcelt
failed_download_right_button  = Mēģināt vēlreiz

stop_download_title           = Apstādināt lejupielādi
stop_download_message         = Vai esat drošs ka vēlaties apstādināt lejupielādi?
stop_download_left_button     = Jā
stop_download_right_button    = Nē

delete_download_title         = Dzēst
delete_download_message       = Vai dzēst {{name}}?
delete_download_left_button   = Atcelt
delete_download_right_button  = Dzēst

delete_all_download_title                      = Dzēst
delete_all_download_left_button                = Atcelt
delete_all_download_right_button               = Dzēst

# LOCALIZATION NOTE: `number' will always be 2 or greater.
delete_all_download_message                    = {[ plural(number) ]}
delete_all_download_message[zero]              = Nedzēst nevienu datni?
delete_all_download_message[one]               = Vai dzēst vienu datni?
delete_all_download_message[two]               = Vai dzēst {{number}} datnes?
delete_all_download_message[few]               = Vai dzēst {{number}} datnes?
delete_all_download_message[many]              = Vai dzēst {{number}} datnes?
delete_all_download_message[other]             = Vai dzēst {{number}} datnes?

unsupported_file_type_download_title           = Nevar atvērt
unsupported_file_type_download_message         = Nav neviena aplikācija kas varēt atvērt {{name}}. Vai vēlaties paturēt datni vai to dzēst?
unsupported_file_type_download_left_button     = Paturēt datni
unsupported_file_type_download_right_button    = Dzēst

file_not_found_download_title                  = Datne netika atrasta
file_not_found_download_message                = Datne netika atrasta ierīcē
file_not_found_download_right_button           = Labi

unmounted_sdcard_2_download_title              = Atmiņas karte jau ir lietošanā
unmounted_sdcard_2_download_message            = Atvienojiet telefonu
unmounted_sdcard_2_download_right_button       = Labi

no_provider_download_title                     = Nevar dalīt
no_provider_download_message                   = Nav atrasts neviens pakalpojumu sniedzējs
no_provider_download_right_button              = Labi

no_memory_download_title                       = SD Karte ir pilna
no_memory_download_message                     = Nav vairs vietas uz SD karte lai saglabāt jūsu lejupielādi. Lūdzu noņemiet vairākas datnes pirms atsākt lejupielādi.
no_memory_download_right_button                = Labi

no_sdcard_found_2_download_title               = Netika atrasta neviena atmiņas karte
no_sdcard_found_2_download_message             = Ievietojiet atmiņas karti vai izvēlieties alternatīvu noklusējuma mediju atrašanas vietu iestatījumos
no_sdcard_found_2_download_right_button        = Labi

file_open_error_download_title                 = Nevar atvērt
file_open_error_download_message               = Ir problēma atverot {{name}}. Vai vēlaties saglabāt datni vai to dzēst?
file_open_error_download_left_button           = Paturēt datni
file_open_error_download_right_button          = Dzēst

restart_download_error                         = Bija kļūda kad pārstartēja lejupielādi. Lūdzu mēģiniet lejupielādēt vēlreiz.
no-downloads                                   = Nav lejupielāžu
# File sizes
byteUnit-B                    = B
byteUnit-KB                   = KB
byteUnit-MB                   = MB
byteUnit-GB                   = GB
byteUnit-TB                   = TB
fileSize                      = {{size}} {{unit}}

percentage={{percentage}} %
partialResult={{partial}} no {{total}}
summary={{date}} - {{status}}

# States
download-stopped              = Apstājās
download-failed               = Lejupielāde neizdevās

open_downloaded_file          = Atvērt
share_downloaded_file         = Dalīt
wallpaper_downloaded_file     = Iestatīt kā darbvirsmas fonu
ringtone_downloaded_file      = Iestatīt kā  as ringtone
cancel_downloaded_file        = Atcelt

